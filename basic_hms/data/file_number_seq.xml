<odoo>
    <!-- Patient Sequence -->
    <record id="seq_patient" model="ir.sequence">
        <field name="name">Patient Sequence</field>
        <field name="code">patient.sequence</field>
        <field name="prefix">1265</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- File Number Sequence -->
    <record id="seq_file_number" model="ir.sequence">
        <field name="name">File Number Sequence</field>
        <field name="code">file.sequence</field>
        <field name="prefix">1265</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>